{
  "name": "sui-cli-web",
  "version": "1.3.3",
  "private": true,
  "description": "Keyboard-first web interface for Sui CLI - Built for First Movers",
  "repository": {
    "type": "git",
    "url": "https://github.com/hien-p/raycast-sui-cli.git",
    "directory": "sui-cli-web"
  },
  "homepage": "https://github.com/hien-p/raycast-sui-cli/tree/master/sui-cli-web#readme",
  "bugs": {
    "url": "https://github.com/hien-p/raycast-sui-cli/issues"
  },
  "keywords": [
    "sui",
    "blockchain",
    "cli",
    "web",
    "first-movers",
    "sui-developers",
    "keyboard-first",
    "community",
    "tier-system"
  ],
  "author": "Harry Phan",
  "license": "MIT",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "npm run dev -w sui-cli-web-server",
    "dev:client": "npm run dev -w @sui-cli-web/client",
    "dev:blog": "npm run dev -w @sui-cli-web/blog",
    "build": "npm run build -w @sui-cli-web/shared && npm run build -w sui-cli-web-server && npm run build -w @sui-cli-web/client && npm run build -w @sui-cli-web/blog && node -e \"require('fs').cpSync('packages/blog/dist','packages/client/dist/blog',{recursive:true})\"",
    "build:server": "npm run build -w sui-cli-web-server",
    "build:client": "npm run build -w @sui-cli-web/client",
    "build:blog": "npm run build -w @sui-cli-web/blog",
    "preview:blog": "npm run preview -w @sui-cli-web/blog",
    "start": "npm run start -w sui-cli-web-server",
    "lint": "npm run lint --workspaces --if-present",
    "clean": "rm -rf packages/*/dist packages/*/node_modules node_modules"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
