{
    "name": "raycast-sui-cli",
    "title": "Sui CLI",
    "description": "A unified command palette for executing Sui CLI commands.",
    "icon": "assets/command-icon.png",
    "author": "hien-p",
    "categories": [
        "Other"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hien-p/raycast-sui-cli.git"
    },
    "commands": [
        {
            "name": "index",
            "title": "Sui Command Palette",
            "subtitle": "Sui CLI",
            "description": "Execute Sui CLI commands",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.79.0",
        "@raycast/utils": "^1.16.3",
        "js-yaml": "^4.1.0"
    },
    "devDependencies": {
        "@raycast/eslint-config": "^1.0.8",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "18.16.0",
        "@types/react": "18.2.0",
        "eslint": "^8.57.0",
        "prettier": "^3.3.3",
        "typescript": "^5.4.5"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "npx @raycast/api@latest publish"
    },
    "overrides": {
        "@types/react": "18.2.0",
        "@types/node": "18.16.0"
    }
}