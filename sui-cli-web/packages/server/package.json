{
  "name": "@sui-cli-web/server",
  "version": "1.1.0",
  "description": "Local server for Sui CLI Web - connects your browser to local Sui CLI",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "sui-cli-web": "./bin/cli.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sui",
    "blockchain",
    "cli",
    "web",
    "wallet",
    "move",
    "crypto"
  ],
  "author": "hien-p",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hien-p/raycast-sui-cli.git"
  },
  "homepage": "https://github.com/hien-p/raycast-sui-cli#readme",
  "bugs": {
    "url": "https://github.com/hien-p/raycast-sui-cli/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "fastify": "^4.26.2",
    "@fastify/cors": "^9.0.1",
    "js-yaml": "^4.1.0",
    "pino-pretty": "^11.0.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.11.30",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5"
  }
}